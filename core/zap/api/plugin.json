{
  "name": "api",
  "description": "A plugin for managing API interactions and integrations using oRPC, TanStack Query and custom hooks.",
  "author": "Alexandre Trotel",
  "dependencies": {
    "packages": [
      "@orpc/client",
      "@orpc/react-query",
      "@orpc/server",
      "@tanstack/react-query",
      "@tanstack/react-query-devtools"
    ],
    "devPackages": ["@tanstack/eslint-plugin-query"],
    "folders": ["src/app/rpc/", "src/rpc/", "src/hooks/rpc/"],
    "plugins": ["errors"]
  }
}
